{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "ext-memcached": "^3.1",
        "apility/laravel-fcm": "^1.4",
        "arcanedev/no-captcha": "^11.0",
        "aws/aws-sdk-php": "^3.133",
        "aws/aws-sdk-php-laravel": "^3.6",
        "barryvdh/laravel-cors": "^2.0",
        "benwilkins/laravel-fcm-notification": "^4.0",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.2",
        "guzzlehttp/guzzle": "^6.5",
        "illuminate/filesystem": "^7.0",
        "intervention/image": "^2.5",
        "kreait/laravel-firebase": "^3.0",
        "laravel/framework": "^7.0",
        "laravel/helpers": "^1.2",
        "laravel/socialite": "^4.4",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "laravelcollective/html": "^6.1",
        "maatwebsite/excel": "^3.1",
        "mariuzzo/laravel-js-localization": "^1.6",
        "mercadopago/dx-php": "2.1.0",
        "mews/captcha": "^3.1",
        "patosmack/mercado-pago": "^1.0@beta",
        "pusher/pusher-php-server": "^4.1",
        "sentry/sentry-laravel": "^1.8",
        "spatie/laravel-cookie-consent": "^2.12",
        "spatie/laravel-sitemap": "^5.7"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "kint-php/kint": "^3.3",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "disable-tls": false
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
